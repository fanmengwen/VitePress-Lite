{
  "name": "docs-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:docker": "vite --config vite.docker.config.ts",
    "build": "vue-tsc -b && vite build",
    "serve:docker": "node docker-server.mjs",
    "build:prerender": "npm run build && npm run prerender:content",
    "prerender": "node scripts/simple-prerender.js",
    "prerender:content": "node scripts/content-prerender.js",
    "prerender:full": "node scripts/prerender.js",
    "preview": "vite preview",
    "preview:prerender": "serve dist-prerender -p 4173",
    "clean": "rm -rf dist && rm -rf dist-prerender && rm -rf node_modules/.cache",
    "lint": "echo 'Linting not configured for docs-site'",
    "type-check": "vue-tsc --noEmit",
    "test:ai": "node test-ai-integration.js",
    "test": "node test-ai-integration.js",
    "test:seo": "node scripts/simple-seo-test.js",
    "test:seo:full": "node scripts/test-seo.js",
    "test:prerender": "node scripts/test-prerender-site.js",
    "test:api": "./scripts/test-api-silence.sh",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:seo": "node scripts/generate-sitemap.js dist"
  },
  "dependencies": {
    "@unhead/vue": "^1.8.10",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "compression": "^1.7.4",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.3",
    "serve": "^14.2.4",
    "vue": "^3.5.17",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-inspect": "^11.3.0",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-pages": "^0.33.1",
    "vue-tsc": "^2.2.12"
  }
}
